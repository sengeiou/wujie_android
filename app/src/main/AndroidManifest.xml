<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.txd.hzj.wjlp">

    <!--打电话-->
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="com.ants.theantsgo.WeApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainAty">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <!-- 线上商城分类(1-1-1 首页 2) -->
        <activity android:name=".mellOnLine.MellOnLineClassifyAty"/>
        <!-- 子分类(1-1-2)二级分类 -->
        <activity android:name=".mellOnLine.SubclassificationAty"/>
        <!-- 分类 (1-2) -->
        <activity android:name=".mellOnLine.AllClassifyAty"/>
        <!--搜索-->
        <activity android:name=".mellOnLine.SearchAty"/>
        <!--商品列表-->
        <activity android:name=".mellOnLine.GoodsListAty"/>
        <!--商家列表-->
        <activity
            android:name=".mellOnLine.MellListAty"
            android:theme="@style/AppTheme"/>
        <!--消息-->
        <activity android:name=".mellOnLine.MessageAty"/>
        <!--消息列表-->
        <activity android:name=".mellOnLine.OrderAndInformMessageListAty"/>
        <!--公告详情-->
        <activity android:name=".mellOnLine.NoticeDetailsAty"/>
        <!--限量购-->
        <activity android:name=".mellOnLine.gridClassify.LimitShoppingAty"/>
        <!--限量购详情-->
        <activity android:name=".mellOnLine.gridClassify.LimitGoodsAty"/>
        <!--分类-->
        <activity android:name=".mellOnLine.gridClassify.TicketZoonAty"/>
        <!--票券详情-->
        <activity android:name=".mellOnLine.gridClassify.TicketGoodsDetialsAty"/>
        <!--拼团详情-->
        <activity android:name=".mellOnLine.gridClassify.GoodLuckDetailsAty"/>
        <!--主题街-->
        <activity android:name=".mellOnLine.gridClassify.ThemeStreetHzjAty"/>
        <!--主题街商品列表-->
        <activity android:name=".mellOnLine.gridClassify.ThemeGoodsListAty"/>
        <!--主题街商品详情-->
        <activity android:name=".mellOnLine.gridClassify.ThemeGoodsDetailsAty"/>

        <!--汽车购-->
        <activity android:name=".mellOnLine.gridClassify.car.CarChenAty"></activity>
        <activity android:name=".mellOnLine.gridClassify.car.CarCommodityChenAty"></activity>
        <!--房产购-->
        <activity android:name=".mellOnLine.gridClassify.hous.HousChenAty"></activity>
        <activity android:name=".mellOnLine.gridClassify.hous.HousDetailsChenAty"></activity>

    </application>

</manifest>