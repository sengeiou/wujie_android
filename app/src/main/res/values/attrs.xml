<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <!--双向水平筛选价格chen-->
    <declare-styleable name="ATDragView">
        <attr name="seek_bg_color" format="color" />
        <attr name="seek_pb_color" format="color" />
        <attr name="seek_ball_solid_color" format="color" />
        <attr name="seek_ball_stroke_color" format="color" />
        <attr name="seek_text_color" format="color" />
        <attr name="seek_text_size" format="dimension" />
    </declare-styleable>
    <declare-styleable name="BannerPointLayout">
        <attr name="styleBannerPointLayout" format="string" />
        <attr name="pointerSize" format="dimension" />
        <attr name="pointerLMargin" format="dimension" />
        <attr name="unPointDrawable" format="reference" />
        <attr name="pointDrawable" format="reference" />
    </declare-styleable>
    <!--双向水平筛选价格，默认颜色chen-->
    <style name="def_dragview">
        <item name="seek_bg_color">#e6e6e6</item>
        <item name="seek_pb_color">@color/theme_color</item>
        <item name="seek_ball_solid_color">#ffffff</item>
        <item name="seek_ball_stroke_color">#e6e6e6</item>
        <item name="seek_text_color">#666666</item>
        <item name="seek_text_size">12sp</item>
    </style>

    <!--张鸿洋FlowLayout-->
    <declare-styleable name="TagFlowLayout">
        <attr name="auto_select_effect" format="boolean"></attr>
        <attr name="max_select" format="integer"></attr>
        <attr name="gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>


    <!--文字跑马灯 chen-->
    <declare-styleable name="ViewTwo">
        <!--文字尺寸-->
        <attr name="scrollTextSize" format="dimension" />
        <!--文字颜色-->
        <attr name="scrollTextColor" format="color" />
        <!--文字内容-->
        <attr name="scrollText" format="string" />
    </declare-styleable>

    <declare-styleable name="ArcProgress">

        <!--初始值-->
        <attr name="arc_progress" format="integer" />
        <!--小数位数-->
        <attr name="decimal_digits" format="integer" />
        <!--弧度-->
        <attr name="arc_angle" format="float" />
        <!--宽度-->
        <attr name="arc_stroke_width" format="dimension" />
        <!--最大值-->
        <attr name="arc_max" format="integer" />
        <!--底色-->
        <attr name="arc_unfinished_color" format="color" />
        <!--画弧度的颜色-->
        <attr name="arc_finished_color" format="color" />
        <attr name="arc_progress_text_size" format="dimension" />
        <attr name="arc_progress_text_color" format="color" />

        <attr name="arc_progress_text_top_text" format="string" />
        <attr name="arc_progress_text_top_size" format="dimension" />
        <attr name="arc_progress_text_top_color" format="color" />


        <attr name="arc_progress_text_bottom_text" format="string" />
        <attr name="arc_progress_text_bottom_size" format="dimension" />
        <attr name="arc_progress_text_bottom_color" format="color" />

        <!--旁边的占位符-->
        <attr name="arc_progress_suffix_text" format="string" />
        <attr name="arc_progress_suffix_text_size" format="dimension" />
        <attr name="arc_progress_suffix_text_color" format="color" />

        <attr name="arc_progress_suffix_text2" format="string" />


        <!--最底部的文字-->
        <attr name="arc_bottom_text" format="string" />
        <attr name="arc_bottom_text_size" format="dimension" />
    </declare-styleable>


    <declare-styleable name="RatingBar">
        <!--尺寸值-->
        <attr name="starImageSize" format="dimension" />
        <!--星星间距-->
        <attr name="starpadding" format="dimension" />
        <!--星星总数-->
        <attr name="starcount" format="integer" />
        <!--空白的星星资源文件值-->
        <attr name="starempty" format="reference" />
        <!--满星资源文件值-->
        <attr name="starFill" format="reference" />
        <!--半星资源文件值-->
        <attr name="starHalf" format="reference" />
        <!--是否可点击boolean值-->
        <attr name="clickable" format="boolean" />
        <!--当前进度float值-->
        <attr name="starStep" format="float" />
        <!--每次进度方式的值，整星还是半星-->
        <attr name="stepSize">
            <enum name="Half" value="0" />
            <enum name="Full" value="1" />
        </attr>
    </declare-styleable>

    <!-- 搜索蓝牙列表展示列表 -->
    <declare-styleable name="MaxHeightView">
        <attr name="mhv_HeightRatio" format="reference|float" />
        <attr name="mhv_HeightDimen" format="reference|dimension" />
    </declare-styleable>

</resources>